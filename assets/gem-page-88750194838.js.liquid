function verticalSlide(setting){
  var showSlide = setting.showSlide ? setting.showSlide : 3;
  var offset = setting.offset ? setting.offset : 0;
    //
    //first set up
    if($module.find('.item').length > showSlide){
      $module.find('.btn-prev-c').css('opacity', '0.3');
      var wp = $module.find('.gf_product-images-list');
      var totalHeight = $(wp).height();
      $(wp).height(totalHeight);
      var item = $module.find('.item');
      for(var i = 0; i < showSlide; i++){
        $(item[i]).addClass('active');
      }
      setHeight()
      //event button
      $module.find('.btn-next-c').on('click', function(e){
        e.preventDefault();
        nextImage();
      })
      $module.find('.btn-prev-c').on('click', function(e){
        e.preventDefault();
        prevImage();
      })
      $module.parent().removeClass('not-slide-t');
    }else{
      $module.parent().addClass('not-slide-t');
    }
    //function
    var transformValue = 0;
    function prevImage(){
      var fistActive = $module.find('.item.active')[0];
      if($(fistActive).prev().length == 0){
        return;
      }
       if($(fistActive).prev().prev().length == 0){
        $module.find('.btn-prev-c').css('opacity', '0.3');
      }
      $module.find('.btn-next-c').css('opacity', '1');
      var lastActive = $module.find('.item.active')[showSlide - 1];
      var prevFistActive = $(fistActive).prev();
      transformValue += $(prevFistActive).outerHeight(true);
      $(wp).css('transform', 'translateY(' + transformValue  +'px)')
      setActivePrev(fistActive, lastActive);
      setHeight();
    }
    function setActivePrev(fistActive, lastActive){
      $(lastActive).removeClass('active');
      $(fistActive).prev().addClass('active');
    }
    function nextImage(){
      var lastActive = $module.find('.item.active')[showSlide -1];
      if($(lastActive).next().length == 0){
        return;
      }
       if($(lastActive).next().next().length == 0){
         $module.find('.btn-next-c').css('opacity', '0.3');
       }
      $module.find('.btn-prev-c').css('opacity', '1');
      var fistActive = $module.find('.item.active')[0];
      transformValue -= $(fistActive).outerHeight(true);
      $(wp).css('transform', 'translateY(' + transformValue  +'px)')
      setActiveNext(fistActive, lastActive);
      setHeight();
    }
    function setActiveNext(fistActive, lastActive){
      $(fistActive).removeClass('active');
      $(lastActive).next().addClass('active');
    }
    function setHeight(){
      var height = offset;
      $module.find('.item.active').each(function(){
        height += $(this).outerHeight(true);
      });
      $module.height(height);
    }
    //resize
    if($module.find('.item').length > showSlide){
       $(window).on('resize', function(){
        $module.find('.btn-prev-c').css('opacity', '0.3');
        $module.find('.btn-next-c').css('opacity', '1');
        var wp = $module.find('.gf_product-images-list');
        var totalHeight = $(wp).height();
        $(wp).height(totalHeight);
        var item = $module.find('.item');
        transformValue = 0
        $(item).removeClass('active');
        for(var i = 0; i < showSlide; i++){
          $(item[i]).addClass('active');
        }
        setHeight()
        $(wp).css('transform', 'translateY(0px)')
      })
    }
    //
}
